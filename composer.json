{
    "type": "project",
    "require": {
        "php": "^7.4",
        "guzzlehttp/psr7": "^1.6",
        "http-interop/http-factory-guzzle": "^1.0",
        "php-http/discovery": "^1.8",
        "psr/http-client": "^1.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.6",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-client": "^5.0",
        "symfony/monolog-bundle": "^3.5",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
