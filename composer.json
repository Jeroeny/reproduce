{
    "type": "project",
    "require": {
        "php": "^7.3",
        "symfony/finder": "^5.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
